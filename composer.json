{
    "name": "ironphp/app",
    "homepage": "https://github.com/ironphp/app",
    "description": "The IronPHP Framework 1.0 application skeleton",
    "type": "project",
    "keywords": [
        "php",
        "framework",
        "mvc",
        "ironphp",
        "frontcontroller",
        "iron-php",
        "ironphp-framework",
        "app",
        "ironphp-app"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ironphp/app/issues",
        "forum": "https://www.facebook.com/IronPHP-Framwork-325690624644002",
        "source": "https://github.com/ironphp/app",
        "email": "gaurangkumarp@gmail.com"
    },
    "authors": [
        {
            "name": "GaurangKumar Parmar",
            "email": "gaurangkumarp@gmail.com",
            "homepage": "https://twitter.com/gaurangkumarp",
            "role": "Creater and Developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ironphp/ironphp": "1.0.0-alpha1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "ironphp": {
            "allow-contrib": true,
            "require": "*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "scripts": {
        "post-root-package-install": "@php -r \"echo 'post-root-package-install'.PHP_EOL;\"",
        "post-autoload-dump": "@php -r \"echo 'post-autoload-dump'.PHP_EOL;\"",
        "post-update-cmd": "@php -r \"echo 'post-update-cmd'.PHP_EOL;\"",
        "post-create-project-cmd": "@php -r \"echo 'post-create-project-cmd'.PHP_EOL;\"",
        "post-install-cmd": "@php -r \"echo 'post-install-cmd'.PHP_EOL;\""
    }
}
